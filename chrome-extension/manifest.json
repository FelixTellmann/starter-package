{
  "manifest_version": 2,
  "name": "FT Up",
  "description": "Quick Launch tools for Felix",
  "version": "1.0.0",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "out/index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [
    "out/*"
  ]
}